{
	"manifest_version": 1,
	"type": "WebApp",
	"meta": {
	  "name": "LoL CS/Win Tracker",
	  "author": "Aaditya R. Bhat",
	  "version": "1.0.0",
	  "description": "Tracks CS per minute & Win Probability in League of Legends",
	  "minimum-overwolf-version": "0.199.0.15",
	  "icon": "IconMouseOver.png",
	  "icon_gray": "IconMouseNormal.png"
	},
	"permissions": [
	  "Games",
	  "GameEvents"
	],
	"data": {
	  "start_window": "in_game",
	  "windows": {
		"in_game": {
		  "file": "index.html",
		  "transparent": true,
		  "click_through": false,
		  "resizable": false,
		  "use_os_windowing": false,
		  "show_in_taskbar": false,
		  "block_topmost": false,
		  "keep_alive": true,
		  "override_display_bounds": "game",
		  "size": {
			"width": 500,
			"height": 300
		  }
		},
		"desktop": {
		  "file": "overlay.html",
		  "transparent": false,
		  "resizable": true,
		  "use_os_windowing": true,
		  "size": {
			"width": 700,
			"height": 400
		  },
		  "min_size": {
			"width": 400,
			"height": 400
		  }
		}
	  },
	  "game_targeting": {
		"type": "dedicated",
		"games": [
		  {
			"class_id": 5426,
			"fullscreen_only": false
		  }
		]
	  }
	},
	"game_events": {
	  "5426": ["match_info", "game_info", "events"]
	}
  }
  